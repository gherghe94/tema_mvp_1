<Window x:Class="MVP1.Views.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVP1.Views"
        mc:Ignorable="d"
        Title="AdminWindow" Height="180" Width="800">
    <DockPanel LastChildFill="False">
        <DockPanel DockPanel.Dock="Top" Margin="10">
            <Button Command="{Binding ManageProductsCommand}"
                    Content="Produse"/>
        </DockPanel>
        <DockPanel DockPanel.Dock="Top" Margin="10">
            <Button Command="{Binding ManageEmployeesCommand}"
                    Content="Angajati"/>
        </DockPanel>
        <DockPanel DockPanel.Dock="Top" Margin="10">
            <Button Command="{Binding ManageTablesCommand}"
                    Content="Mese"/>
        </DockPanel>
    </DockPanel>
</Window>
